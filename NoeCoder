#include<bits/stdc++.h>
using namespace std;
int main(){
    int n,m;
    cin>>n>>m;
    int a[n],b[m];
    int cot1=0,cot2=0;
    int min=99999;  //设置最小值
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
        cot1+=a[i];
    }
    for(int i=0;i<m;i++){
        cin>>b[i];
    }
    int a1,b1;
    for(int i=0;i<m;i++){
        cot2=b[i];
        for(int j=i+1;j<m;j++){
            cot2+=b[j];
            if(abs(cot1-cot2)<min)
            {
                min=abs(cot1-cot2);
                a1=i;
                b1=j;
            }
        }
        }
    for(int i=a1;i<=b1;i++)
    {
        cout<<b[i];
        cout<<' ';
    }
    return 0;
    }
